using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp4am
{
    class Program
    {
        static int Exp(string exp1, string exp2)
        {
            int result = 0;

            for (int i = 0; i < Math.Min(exp1.Length, exp2.Length); i++)
            {
                result += (int)exp1[i] * (int)exp2[i];
            }
            if (exp1.Length > exp2.Length)
            {
                for (int i = exp2.Length; i < exp1.Length; i++)
                {
                    result += (int)exp1[i];
                }
            }
            else
            {
                for (int i = exp1.Length; i < exp2.Length; i++)
                {
                    result += (int)exp2[i];
                }
            }
            return result;
        }
        static void Main(string[] args)
        {
            string expression1 = Console.ReadLine();
            string expression2 = Console.ReadLine();

            Console.WriteLine(Exp(expression1,expression2));
        }
    }
}
